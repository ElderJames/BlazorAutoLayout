<Router AppAssembly="@typeof(Client._Imports).Assembly" >
    <Found Context="routeData">
        <CascadingValue Value="new RouteDataInfo(routeData)">
            <RouteView RouteData="@routeData" DefaultLayout="@typeof(Client.Pages.ClientLayout)" />
            <FocusOnNavigate RouteData="@routeData" Selector="h1" />
        </CascadingValue>
    </Found>
</Router>
